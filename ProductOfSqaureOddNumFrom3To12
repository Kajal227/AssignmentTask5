package com.tka.AssignmentFive;

public class ProductOfSqaureOddNumFrom3To12 
{

	public static void main(String[] args)
	{
	
		long product=1;//product kadaych mhanl ki long datatype gheych and value intititalize 1 thevaych.
		int sqaure=0;
		
		for(int num=3;num<=12;num++)
		{
			if(num%2!=0)
			{
				sqaure=num*num;
				product*=sqaure;
			}
		}
		
		System.out.println(product);
	
	}
	}
output:-108056025
